{
    "comments": {
      "lineComment": "#"
    },
    "brackets": [
      ["{", "}"],
      ["[", "]"],
      ["(", ")"]
    ],
    "autoClosingPairs": [
      { "open": "{", "close": "}" },
      { "open": "[", "close": "]" },
      { "open": "(", "close": ")" },
      { "open": "\"", "close": "\"" },
      { "open": "\"\"\"", "close": "\"\"\"" }
    ],
    "indentationRules": {
      "increaseIndentPattern": "^.*:\\s*$",
      "decreaseIndentPattern": "^\\s*$"
    }
  }